[core]
        vim
        repositoryformatversion = 0
        filemode = true
        bare = false
        logallrefupdates = true
        ignorecase = true
        precomposeunicode = true
[alias]
        s= status
        a= add .
        b= branch
        c= commit -m
        p= push https://github.com/mohanenm/streamCloud
        pb = push https://enmohan@bitbucket.org/enmohan/cs310f18enm.git
[include]
        path = ../.gitconfig

[color]
    diff = auto
    status = auto
    branch = auto
